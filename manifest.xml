<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
  <Id>e63cdf73-91cd-4534-96f0-5862e5e31029</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Slider</ProviderName>
  <DefaultLocale>ko-KR</DefaultLocale>
  <DisplayName DefaultValue="장표관리자 Slider"/>
  <Description DefaultValue="장표를 관리하는 가장 효율적인 도구"/>
  <IconUrl DefaultValue="https://wookji-yoon.github.io/myproject/assets/icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://wookji-yoon.github.io/myproject/assets/icon-80.png"/>
  <SupportUrl DefaultValue="https://slider10.notion.site/"/>
  <AppDomains>
    <AppDomain>https://slider10.notion.site</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Presentation"/>
  </Hosts>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://wookji-yoon.github.io/myproject/taskpane.html"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Presentation">
        <DesktopFormFactor>
          <GetStarted>
            <Title resid="GetStarted.Title"/>
            <Description resid="GetStarted.Description"/>
            <LearnMoreUrl resid="GetStarted.LearnMoreUrl"/>
          </GetStarted>
          <FunctionFile resid="Commands.Url"/>
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <OfficeTab id="TabHome">
              <Group id="CommandsGroup">
                <Label resid="CommandsGroup.Label"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>
                <Control xsi:type="Button" id="TaskpaneButton">
                  <Label resid="TaskpaneButton.Label"/>
                  <Supertip>
                    <Title resid="TaskpaneButton.Label"/>
                    <Description resid="TaskpaneButton.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>ButtonId1</TaskpaneId>
                    <SourceLocation resid="Taskpane.Url"/>
                  </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <bt:Image id="Icon.16x16" DefaultValue="https://wookji-yoon.github.io/myproject/assets/icon-16.png"/>
        <bt:Image id="Icon.32x32" DefaultValue="https://wookji-yoon.github.io/myproject/assets/icon-32.png"/>
        <bt:Image id="Icon.80x80" DefaultValue="https://wookji-yoon.github.io/myproject/assets/icon-80.png"/>
      </bt:Images>
      <bt:Urls>
        <bt:Url id="GetStarted.LearnMoreUrl" DefaultValue="https://slider10.notion.site/"/>
        <bt:Url id="Commands.Url" DefaultValue="https://wookji-yoon.github.io/myproject/commands.html"/>
        <bt:Url id="Taskpane.Url" DefaultValue="https://wookji-yoon.github.io/myproject/taskpane.html"/>
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="GetStarted.Title" DefaultValue="장표관리자 Slider"/>
        <bt:String id="CommandsGroup.Label" DefaultValue="Slider"/>
        <bt:String id="TaskpaneButton.Label" DefaultValue="장표관리자 Slider"/>
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="GetStarted.Description" DefaultValue="장표관리자 Slider를 실행 중입니다"/>
        <bt:String id="TaskpaneButton.Tooltip" DefaultValue="장표관리자 Slider를 실행합니다"/>
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>